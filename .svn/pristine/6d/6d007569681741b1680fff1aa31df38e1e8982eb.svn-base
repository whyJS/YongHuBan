		<script type="text/javascript">
			$(document).ready(function() {
				function hidden() {
					$('#firm').css({
						'display': 'block'
					})
					$('#bt1 h1').removeClass('text-active')
					$('#bt1 h1').addClass('text-active')
					$('#business').css({
						'display': 'none'
					})
					$('#bt2 h1').removeClass('text-active')
				}

				function hidden2() {
					$('#firm').css({
						'display': 'none'
					})
					$('#bt1 h1').removeClass('text-active')
					$('#business').css({
						'display': 'block'
					})
					$('#bt2 h1').removeClass('text-active')
					$('#bt2 h1').addClass('text-active')
				}
				var a = 1
				if(a == 1) {
					hidden()
				} else {
					hidden2()
				}
				$('#bt1').click(function() {
					hidden()
				})
				$('#bt2').click(function() {
					hidden2()
				})
			})
