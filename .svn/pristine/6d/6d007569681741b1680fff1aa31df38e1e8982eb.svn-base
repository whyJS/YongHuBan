			}
			
			.news-wrapper {
				width: 1200px;
				margin: 0 auto;
				margin-top: 50px;
				background: #ffffff;
				box-sizing: border-box;
				-webkit-box-sizing: border-box;
				margin-bottom: 150px;
			}
			/*pc端*/
			
			.max-wrapper {
				box-sizing: border-box;
				padding-top: 40px;
				padding-left: 40px;
			}
			
			.head-wrapper {
				display: inline-block;
			}
			
			.head-wrapper-active {
				padding-right: 46px;
			}
			
			.text {
				margin: 0;
				padding: 0;
				width: 156px;
				font-size: 36px;
				box-sizing: border-box;
				padding-bottom: 15px;
				font-weight: 400;
				border-bottom: 6px solid #c0c0c0;
				text-align: center;
			}
			
			.text-active {
				border-bottom: 6px solid #00325c;
			}
			
			.text:hover {
				border-bottom: 6px solid #00325c;
			}
			/*内容*/
			
			.max-content {
				list-style-type: none;
				margin: 0;
				padding: 0;
			}
			
			.max-content li {
				padding-top: 32px;
				padding-bottom: 32px;
				border-bottom: 1px solid #e3e3e3;
				box-sizing: border-box;
				display: flex;
				display: -webkit-flex;
			}
			
			.max-content li:last-child {
				border-bottom: 1px solid #FFFFFF;
			}
			
			.max-content li img {
				width: 300;
				height: 220px;
			}
			
			.max-content-news {
				box-sizing: border-box;
				flex: 1;
				padding-left: 60px;
				position: relative;
			}
			
			.content-h1 {}
			
			.content-p {
				font-size: 16px;
				margin-top: 15px;
			}
			
			.content-a {
				display: block;
				width: 110px;
				box-sizing: border-box;
				line-height: 40px;
				height: 40px;
				background: #074e9c;
				color: #FFFFFF;
				text-align: center;
				position: absolute;
				bottom: 0px;
				text-decoration: none;
			}
			/*移动端*/
			
			@media only screen and (min-width: 991px) and (max-width: 1200px) {
